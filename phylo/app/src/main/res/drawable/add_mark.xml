<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="30dp"
        android:height="3dp">
        <shape android:shape="rectangle">
                <solid android:color="@color/white"/>
        </shape>
    </item>

    <item
        android:width="30dp"
        android:height="3dp">
        <rotate
            android:fromDegrees="90">
            <shape android:shape="rectangle">
                <solid android:color="@color/white"/>
            </shape>
        </rotate>
    </item>
</layer-list>